{
  "rewrites": [
    {
      "source": "/smartflow/(.*)",
      "destination": "/smartflow/$1"
    },
     {
      "source": "/chatbot/(.*)",
      "destination": "/chatbot/$1"
    },
     {
      "source": "/structure/(.*)",
      "destination": "/structure/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
